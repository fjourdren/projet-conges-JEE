<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>dayoff_manager</display-name>

    <servlet>
        <display-name>HelloWorld</display-name>
        <servlet-name>HelloWorld</servlet-name>
        <servlet-class>fr.enssat.dayoff_manager.servlets.HelloWorldServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>HelloWorld</servlet-name>
        <url-pattern>/HelloWorld</url-pattern>
    </servlet-mapping>

    <servlet>
        <display-name>TestDAOServlet</display-name>
        <servlet-name>TestDAOServlet</servlet-name>
        <servlet-class>fr.enssat.dayoff_manager.servlets.TestDAOServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>TestDAOServlet</servlet-name>
        <url-pattern>/TestDAOServlet</url-pattern>
    </servlet-mapping>



